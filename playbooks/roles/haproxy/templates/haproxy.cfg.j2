global
    log /dev/log local0
    maxconn 4096

defaults
    log global
    mode http
    timeout connect 5000ms
    timeout client 50000ms
    timeout server 50000ms

frontend http_front
    bind *:80
    default_backend web_backend

backend web_backend
    balance roundrobin
    server web1 192.168.1.51:80 check
    server web2 192.168.1.52:80 check
